{{ header }}
{{ column_left }}
<div id="content">
	<div class="hidden">
		<span class="enabled">{{ text_enabled }}</span>
		<span class="disabled">{{ text_disabled }}</span>
		<span class="yes">{{ text_yes }}</span>
		<span class="no">{{ text_no }}</span>
	</div>
	<div id="product-extra-wrapper" class="product-extra-wrapper {% if (ajaxed == true) %} ajaxed {% endif %}">
		<div class="page-header">
			<div class="container-fluid">
				{# Action buttons #}
				<div class="pull-right">
					{% if (languages) %}
						<div class="btn-group language-selector">
							<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
								Language:
								<span id="selected-language">
									{{ selected_lang }}
								</span>
								<span class="caret"></span>
							</button>

							<ul class="dropdown-menu language-selection" role="menu">
								{% for language in languages %}
									<li>
										<a
											href="#"
											data-code="{{ language.name }}"
											data-id="{{ language.language_id }}">{{ language.name }}
										</a>
									</li>
								{% endfor %}
							</ul>
						</div>
					{% endif %}

					<a
						href="{{ insert }}"
						class="btn btn-primary insert-product"
						data-toggle="tooltip"
						title="{{ button_insert }}"
					>
						<i class="fa fa-plus"></i>
					</a>

					<button
						type="button"
						formaction="{{ copy }}"
						data-toggle="tooltip"
						title="{{ button_copy }}"
						class="btn btn-default btn-ipe-action copy"
					>
						<i class="fa fa-copy"></i>
					</button>
					
					<button
						type="button"
						formaction="{{ delete }}"
						data-toggle="tooltip"
						title="{{ button_delete }}"
						class="btn btn-danger btn-ipe-action delete"
					>
						<i class="fa fa-trash-o"></i>
					</button>
					
					<a
						class="btn btn-info copy-default-product"
						href="{{ copy }}"
						data-toggle="tooltip"
						title="{{ copy_default_product }}"
					>
						<i class="fa fa-file-o"></i>
					</a>
					
					<a
						onclick="editDefaultProduct()"
						class="btn btn-info default-product"
						href="index.php?route=catalog/product/edit&user_token={{ user_token }}&product_id="
						data-toggle="tooltip"
						title="{{ edit_default_product }}"
					>
						<i class="fa fa-pencil-square-o"></i>
					</a>
					<button
						class="btn btn-warning columns-button"
						title="{{ column_switch }}"
						data-toggle="tooltip"
					>
						<i class="fa fa-check-square-o"></i>
					</button>
					
					<button
						class="btn btn-default settings-button"
						title="{{ settings_switch }}"
						data-toggle="tooltip"
					>
						<i class="fa fa-cogs"></i>
					</button>
					<!--view-hook-buttons-top--> 
				</div>
				
				{# Title & breadcrumbs #}
				<h1>{{ heading_title }}</h1>
				<ul class="breadcrumb">
					{% for breadcrumb in breadcrumbs %}
						<li>
							<a href="{{ breadcrumb.href }}">
								{{ breadcrumb.text }}
							</a>
						</li>
					{% endfor %}
				</ul>
			</div>
		</div>
